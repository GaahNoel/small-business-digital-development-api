{
  "name": "small-business-digital-development-api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "devDependencies": {
    "@faker-js/faker": "^6.1.1",
    "@swc/core": "^1.2.156",
    "@swc/jest": "^0.2.20",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/nodemailer": "^6.4.4",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "^7.12.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.0.0",
    "jest": "^27.5.1",
    "prisma": "^3.11.0",
    "supertest": "^6.2.2",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.14.1",
    "typescript": "^4.6.2"
  },
  "scripts": {
    "test": "jest",
    "lint": "eslint src",
    "local": "ts-node-dev -r tsconfig-paths/register --exit-child --inspect --transpile-only --ignore-watch node_modules --respawn src/main/server.ts",
    "dev:up": "docker-compose up",
    "dev:down": "docker-compose down",
    "db": "docker-compose up -d db",
    "build": "rm -rf dist && tsc --project tsconfig.build.json",
    "start": "node dist/main/server.js",
    "migrations:run": "prisma migrate deploy --schema ./prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^3.11.0",
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "module-alias": "^2.2.2",
    "nodemailer": "^6.7.3",
    "yarn-audit-fix": "^9.2.1"
  },
  "engines": {
    "node": "16.x"
  },
  "_moduleAliases": {
    "@": "src"
  }
}
